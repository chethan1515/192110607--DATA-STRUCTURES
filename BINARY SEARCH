#include <stdio.h>
int main()
{
	int arr[100],i,high,low,x,n,mid;
	printf("enter the no.of numbers in array");
	scanf("%d",&n);
	for(i=0;i<=n;i++)
	{
		printf("enter the numbers in array :");
		scanf("%d",&arr[i]);
	}
	printf("enter the number to search in array");
	scanf("%d",&x);	
	low=0;
	high=n-1;
	mid=(low+high)/2;
	while(low<=high)
	{
		if(arr[mid]<x)
		low=mid+1;
		else if(arr[mid]==x)
		{
		printf("%d find at %d.n",x,mid+1);
		break;	
		}
		else
		high=mid-1;
		mid=(low+high)/2;
		
	}
	if(low>high)
	printf("no data found");
}
